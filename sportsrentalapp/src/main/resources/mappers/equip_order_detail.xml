<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- name space는 BookRepo의 fully qualified name으로 설정한다. -->
<mapper namespace="com.ssafy.sports.model.dao.EquipDetailDao">
    <insert id="insert" parameterType="EquipOrderDetail">
        insert into equip_order_detail
        (equip_order_id, equip_id, quantity)
        values
        (#{equipOrderId}, #{equipId}, #{quantity})
    </insert>

</mapper>